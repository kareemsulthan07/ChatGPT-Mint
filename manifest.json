{
    "manifest_version": 3,
    "name": "ChatGPT Mint",
    "version": "1.0",
    "description": "Tools for chatgpt.com",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://chatgpt.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}